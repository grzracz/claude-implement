{
  "name": "claude-implement",
  "displayName": "claude-implement",
  "description": "Implement function stub with Claude Code through a button press and selection",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
            "command": "claude-implement.implement",
            "title": "Claude: Implement Function"
        }
    ],
    "keybindings": [
        {
            "command": "claude-implement.implement",
            "key": "ctrl+alt+i",
            "mac": "cmd+alt+i",
            "when": "editorTextFocus && editorHasSelection"
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grzracz/claude-implement"
  },
  "keywords": [
    "vscode",
    "claude",
    "implementation",
    "ai",
    "function",
    "stub"
  ],
  "author": "grzracz",
  "publisher": "grzracz",
  "license": "MIT"
}
